  "components": {
    "schemas": {
      "AuthDto": {
        "required": [
          "apiKey",
          "email",
          "firstName",
          "lastName",
          "mobile"
        ],
        "type": "object",
        "properties": {
          "apiKey": {
            "maxLength": 100,
            "minLength": 6,
            "type": "string"
          },
          "firstName": {
            "maxLength": 50,
            "minLength": 2,
            "pattern": "^[a-zA-Z\\s]+$",
            "type": "string"
          },
          "lastName": {
            "maxLength": 50,
            "minLength": 2,
            "pattern": "^[a-zA-Z\\s]+$",
            "type": "string"
          },
          "email": {
            "maxLength": 254,
            "minLength": 0,
            "type": "string",
            "format": "email"
          },
          "mobile": {
            "minLength": 1,
            "type": "string",
            "format": "tel"
          }
        },
        "additionalProperties": false
      },
      "CertificateEntryDto": {
        "required": [
          "certificateName",
          "details"
        ],
        "type": "object",
        "properties": {
          "certificateName": {
            "maxLength": 200,
            "minLength": 0,
            "type": "string"
          },
          "details": {
            "maxLength": 2000,
            "minLength": 0,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "EducationEntryDto": {
        "required": [
          "date",
          "details",
          "institutionName",
          "location"
        ],
        "type": "object",
        "properties": {
          "institutionName": {
            "maxLength": 200,
            "minLength": 0,
            "type": "string"
          },
          "date": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "location": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "details": {
            "maxLength": 1000,
            "minLength": 0,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "JobDto": {
        "required": [
          "jobDescription",
          "jobName",
          "publicId"
        ],
        "type": "object",
        "properties": {
          "publicId": {
            "minLength": 1,
            "type": "string"
          },
          "jobName": {
            "maxLength": 100,
            "minLength": 2,
            "type": "string"
          },
          "jobDescription": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "ProjectEntryDto": {
        "required": [
          "description",
          "projectName"
        ],
        "type": "object",
        "properties": {
          "projectName": {
            "maxLength": 200,
            "minLength": 0,
            "type": "string"
          },
          "description": {
            "maxLength": 2000,
            "minLength": 0,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "SkillsEntryDto": {
        "required": [
          "skillName"
        ],
        "type": "object",
        "properties": {
          "skillName": {
            "maxLength": 2000,
            "minLength": 0,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "UserDto": {
        "required": [
          "email",
          "firstName",
          "jobField",
          "lastName",
          "linkedInUrl",
          "location",
          "mobile",
          "portfolioUrl",
          "province",
          "userSummary"
        ],
        "type": "object",
        "properties": {
          "firstName": {
            "maxLength": 50,
            "minLength": 2,
            "pattern": "^[a-zA-Z\\s]+$",
            "type": "string"
          },
          "lastName": {
            "maxLength": 50,
            "minLength": 2,
            "pattern": "^[a-zA-Z\\s]+$",
            "type": "string"
          },
          "email": {
            "maxLength": 254,
            "minLength": 0,
            "type": "string",
            "format": "email"
          },
          "mobile": {
            "minLength": 1,
            "type": "string",
            "format": "tel"
          },
          "location": {
            "maxLength": 100,
            "minLength": 2,
            "type": "string"
          },
          "province": {
            "maxLength": 50,
            "minLength": 2,
            "type": "string"
          },
          "jobField": {
            "maxLength": 100,
            "minLength": 2,
            "type": "string"
          },
          "portfolioUrl": {
            "maxLength": 500,
            "minLength": 0,
            "type": "string"
          },
          "linkedInUrl": {
            "maxLength": 500,
            "minLength": 0,
            "type": "string"
          },
          "userSummary": {
            "maxLength": 1500,
            "minLength": 0,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "WorkEntryDto": {
        "required": [
          "companyName",
          "date",
          "details",
          "location"
        ],
        "type": "object",
        "properties": {
          "companyName": {
            "maxLength": 200,
            "minLength": 0,
            "type": "string"
          },
          "date": {
            "maxLength": 200,
            "minLength": 0,
            "type": "string"
          },
          "location": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "details": {
            "maxLength": 2000,
            "minLength": 0,
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    }
